{
  "name": "cypress-documentation",
  "version": "2.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypress-io/docs.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "build:profile": "next build --profile",
    "build:debug": "next build --debug",
    "copy": "bash ./scripts/clone-current-docs.sh",
    "dev": "next dev",
    "dev:watch": "next-remote-watch ./content",
    "changed-files-broken-link-checker:ci": "node scripts/changedFilesBrokenLinkChecker.js",
    "broken-link-checker:prod": "node scripts/recursiveBrokenLinkChecker.js",
    "serve:dist": "node scripts/server.js",
    "lint:markdown": "prettier --write '**/*.md'",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "pre-commit": "yarn types && pretty-quick --staged && yarn run lint && yarn run stop-only",
    "prettier": "prettier --write '**/**.{ts,js,tsx}' '*.{json,md,yml}'",
    "start": "next start",
    "test": "./node_modules/.bin/cypress open",
    "test:unit": "mocha -r ts-node/register -r tsconfig-paths/register 'tests/**/*.ts'",
    "transform": "node scripts/transform.js",
    "types": "tsc --noEmit",
    "make-empty-commit": "node node_modules/make-empty-github-commit/bin/empty.js --repo cypress-io/docs",
    "postinstall": "husky install",
    "update-algolia-index": "yarn create-search-index && ts-node scripts/updateAlgoliaIndex.ts"
  },
  "lint-staged": {
    "*.+(ts|tsx)": "eslint --fix",
    "*.+(css|tsx)": "stylelint",
    "*.css": "stylelint --fix",
    "*.+(ts|tsx|json|css|md|mdx)": "prettier --write"
  },
  "dependencies": {
    "@docsearch/react": "^3.0.0-alpha.41",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@fullstory/browser": "^1.4.9",
    "@sanity/client": "^2.8.0",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@zeit/next-css": "^1.0.1",
    "deepdash": "^5.3.5",
    "next": "^12.0.4",
    "prism-themes": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-pro-sidebar": "^0.7.1",
    "rehype-slug": "^5.0.0",
    "remark-directive": "^1.0.1",
    "replace-in-file": "^6.2.0",
    "unist-util-visit": "^2.0.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@cypress/env-or-json-file": "^2.0.0",
    "@cypress/eslint-plugin-dev": "^5.1.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@mapbox/rehype-prism": "^0.8.0",
    "@netlify/plugin-sitemap": "^0.8.1",
    "@percy/cypress": "^3.1.1",
    "@types/mocha": "^9.0.0",
    "@types/react": "^17.0.35",
    "autoprefixer": "^10.4.0",
    "babel-eslint": "^10.1.0",
    "broken-link-checker": "^0.7.8",
    "chalk": "^4.1.0",
    "common-tags": "^1.8.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.0",
    "eslint": "^8.2.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "gray-matter": "^4.0.3",
    "husky": "^5.1.1",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "make-empty-github-commit": "^1.2.1",
    "markdown-toc-unlazy": "^1.0.1",
    "mocha": "^9.0.3",
    "netlify-cli": "^3.5.0",
    "next-mdx-remote": "^3.0.8",
    "next-sitemap": "^1.6.203",
    "postcss": "^8.3.11",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.5",
    "remark": "^13.0.0",
    "request-promise": "^4.2.6",
    "strip-markdown": "^4.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
